.container{
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    width: 25%;
}
.imageBox{
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 20rpx;
    width: 100rpx;
    height: 100rpx;
    border-radius: 20rpx;
    background-color: #ffffff;
}
.image{
    width:60rpx;
    height: 60rpx;
    background-size: 100% 100%;
}
.text{
    color: #353535;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    line-height: 40rpx;
}